<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DataCleansing.Dashboard">

   <children>
      <Pane prefHeight="200.0" prefWidth="1000.0" style="-fx-background-color: #2D75F8;">
         <children>
            <Label layoutX="55.0" layoutY="46.0" prefHeight="109.0" prefWidth="296.0" text="Dashboard" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="53.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <GridPane layoutX="108.0" layoutY="324.0" prefHeight="293.0" prefWidth="829.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#openDataCleansing" prefHeight="200.0" prefWidth="200.0">
               <graphic>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/dataclean.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#openReport" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
               <graphic>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/report.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#exit" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2">
               <graphic>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/exit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </GridPane>
      <Label ellipsisString="sdsd" layoutX="141.0" layoutY="617.0" text="Data Cleansing">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label ellipsisString="sdsd" layoutX="449.0" layoutY="617.0" text="Report">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label ellipsisString="sdsd" layoutX="751.0" layoutY="617.0" text="EXIT">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
