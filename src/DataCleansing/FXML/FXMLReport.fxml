                <TableColumn fx:id="columnEmployee" minWidth="4.0" prefWidth="122.0" text="Employee name" />
            <TableColumn fx:id="columnComment" prefWidth="220.0" text="Comment" />
            <TableColumn fx:id="columnHDDSerial" prefWidth="115.0" text="HDD Serial number" />
